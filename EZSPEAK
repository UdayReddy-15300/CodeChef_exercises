t=int(input())
t_list=list()
vowels=['a','e','i','o','u']
for i in range(0,t):
    n=int(input())
    s=input()
    count=0
    for i in s:
        if i not in vowels:
            count=count+1
        elif i in vowels:
            count=0
        if count>=4:
            t_list.append('no')
            break
    if count<4:
        t_list.append('yes')

for i in t_list:
    print(i)
